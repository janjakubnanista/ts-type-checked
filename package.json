{
  "name": "ts-type-checked",
  "version": "0.4.0",
  "description": "Type checking utilities for TypeScript.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build:source": "tsc",
    "build": "yarn build:source && yarn copy:meta && yarn lint:build",
    "clean": "rm -rf dist && rm -rf sandbox && rm -rf test/node_modules/ts-type-checked && rm -rf examples/node_modules/ts-type-checked",
    "copy:meta": "./scripts/copy-metadata.sh",
    "lint:source": "eslint . --fix --ext .ts,.tsx",
    "lint:build": "eslint ./dist --fix --ext .js,.ts",
    "test:examples": "cd examples && yarn && yarn build && yarn start",
    "test:version": "./scripts/test.sh",
    "test:versions": "./scripts/test-all-versions.sh",
    "test": "yarn test:versions && yarn test:examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janjakubnanista/ts-type-checked.git"
  },
  "keywords": [
    "TypeScript",
    "type checking",
    "type checks",
    "duck typing",
    "type guards"
  ],
  "author": "Jan Jakub Nanista <jan.jakub.nanista@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janjakubnanista/ts-type-checked/issues"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "transformer/**/*.js",
    "transformer/**/*.d.ts"
  ],
  "homepage": "https://github.com/janjakubnanista/ts-type-checked#readme",
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "fast-check": "^1.25.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "tslib": "^2.0.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "typescript": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}(x)?": [
      "eslint --fix"
    ]
  }
}
